package Graphics;

public class Image {
	private final int[] Pixels;
	private final int WIDTH, HEIGHT;

	public Image(int w, int h, String p) {
		WIDTH = w;
		HEIGHT = h;
		Pixels = new int[w * h];
	}

	private void Load(String path) {
	}

	public int[] getPixels() {
		return Pixels;
	}

	public int getWidth() {
		return WIDTH;
	}

	public int getHeight() {
		return HEIGHT;
	}
}