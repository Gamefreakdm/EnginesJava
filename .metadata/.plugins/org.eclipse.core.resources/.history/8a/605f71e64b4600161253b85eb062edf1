package Main;

import Graphics.Render.Screen;
import Input.KeyHandler;
import Input.MouseHandler;

public class Game {
	private String gs;
	private double Time = 0;
	private final Screen screen;
	private final KeyHandler KH;
	private final MouseHandler MH;

	public Game(KeyHandler kh, MouseHandler mh, Screen sc) {
		KH = kh;
		screen = sc;
		MH = mh;
	}

	public void Update(KeyHandler kh, MouseHandler mh, Screen sc) {
	}

	public void Render() {
	}

	public double getTime() {
		return Time;
	}

	public String getGs() {
		return gs;
	}

	public void setGs(String gs) {
		this.gs = gs;
	}
}